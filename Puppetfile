forge "http://forge.puppetlabs.com"

mod 'puppetlabs/stdlib'
mod 'puppetlabs/apache'

mod "apt",
  :git => "git://github.com/puppetlabs/puppetlabs-apt.git"

mod "wget",
  :git => "git://github.com/maestrodev/puppet-wget.git"

mod "jenkins",
  :git => "git://github.com/excellaco/puppet-jenkins.git"

mod "nexus",
  :git => "git://github.com/excellaco/puppet-nexus.git"

mod "sonar",
  :git => "git://github.com/maestrodev/puppet-sonar.git"

# Git Servers
mod "gitolite",
  :git => "git://github.com/jfryman/puppet-gitolite.git"
mod "gitblit",
	:git => "git://github.com/excellaco/puppet-gitblit.git"